// schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("postgres://postgres:password@db:5432/mydb")
}

generator client {
  provider = "prisma-client-js"
  output   = "./src/prisma/client"
}

model User {
  id       Int      @id @default(autoincrement())
  username String
  email    String   @unique
  password String
}
